{
  "version": "2.0",
  "app_name": "<app-name>",
  "stages": {
    "staging": {
      "environment_variables": {
        "STAGE": "staging",
        "APP_NAME": "<app-name>"
      },
      "api_gateway_stage": "staging",
      "automatic_layer": true,
      "autogen_policy": false
    },
    "prod": {
      "environment_variables": {
        "STAGE": "prod",
        "APP_NAME": "<app-name>"
      },
      "api_gateway_stage": "prod",
      "automatic_layer": true,
      "autogen_policy": false
    }
  }
}